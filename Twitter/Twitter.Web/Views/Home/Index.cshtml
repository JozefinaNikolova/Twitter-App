@model IEnumerable<Twitter.Web.Models.TweetViewModel>

<div class="jumbotron">
    <h1>Twitter</h1>
</div>

@foreach (var item in Model)
{
    <div class="panel panel-default">
        <div class="panel-body">
            <p><strong>@item.PostedBy</strong></p>
            <p>@item.Content</p>
            <p>@item.Url</p>

            <button type="button" class="btn btn-success">Favourite</button>
            <button type="button" class="btn btn-success">Retweet</button>
            <button type="button" class="btn btn-success">Reply</button>
            <button type="button" class="btn btn-success">Report</button>
            <button type="button" class="btn btn-success">Share</button>
        </div>
    </div>
}


